{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Express Server",
      "runtimeExecutable": "node",
      "program": "${workspaceFolder}/index.js", // Express 애플리케이션의 진입점 파일로 바꿔야 함
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
  ]
}
